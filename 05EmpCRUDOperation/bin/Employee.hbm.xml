<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.sk.emp.domain.Employee" table="emp">
		<id column="empno" name="empno" type="java.lang.Integer">
			<generator class="assigned" />
		</id>
		<property name="ename" type="java.lang.String" />
		<property name="job" type="java.lang.String" />
		<property name="mgr" type="java.lang.Integer" />
		<property name="hiredate" type="java.sql.Date" />
		<property name="sal" type="java.lang.Float" />
		<property name="comm" type="java.lang.Integer" />
		<property name="deptno" type="java.lang.Integer" />
	</class>
</hibernate-mapping>